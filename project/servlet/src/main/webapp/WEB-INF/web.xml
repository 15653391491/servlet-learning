<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <filter>
        <filter-name>FilterTest</filter-name>
        <filter-class>filter.FilterTest</filter-class>
        <init-param>
            <param-name>Site</param-name>
            <param-value>菜鸟教程</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>FilterTest</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <!--  sql  -->
    <servlet>
        <servlet-name>sqlTest</servlet-name>
        <servlet-class>servlet.SqlTest</servlet-class>
        <init-param>
            <param-name>jdbcDriver</param-name>
            <param-value>com.mysql.cj.jdbc.Driver</param-value>
        </init-param>
        <init-param>
            <param-name>url</param-name>
            <param-value>jdbc:mysql://124.70.44.128:3306/account</param-value>
        </init-param>
        <init-param>
            <param-name>user</param-name>
            <param-value>root</param-value>
        </init-param>
        <init-param>
            <param-name>password</param-name>
            <param-value>mysql</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>sqlTest</servlet-name>
        <url-pattern>/sql</url-pattern>
    </servlet-mapping>
    <!--  servlet 测试  -->
    <servlet>
        <servlet-name>HelloWorld</servlet-name>
        <servlet-class>servlet.Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloWorld</servlet-name>
        <url-pattern>/HelloWorld</url-pattern>
    </servlet-mapping>
    <!--  mybatis 测试  -->
    <servlet>
        <servlet-name>mybatis</servlet-name>
        <servlet-class>servlet.MybatisTest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>mybatis</servlet-name>
        <url-pattern>/mybatis</url-pattern>
    </servlet-mapping>
</web-app>
